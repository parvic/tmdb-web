character: string;\n  profile_path: string;\n}\n\ninterface Crew {\n  id: number;\n  credit_id: string;\n  name: string;\n  job: string;\n  profile_path: string;\n}\n\ninterface Social {\n  facebook_id: string;\n  instagram_id: string;\n  twitter_id: string;\n}\n\nconst Movie = () => {\n  const { params } = useRouteMatch<MovieParams>();\n  const [movieDetail, setMovieDetail] = useState<Movie>();\n  const [movieSocial, setMovieSocial] = useState<Social>();\n  const [movieCast, setMovieCast] = useState<Cast[]>([]);\n  const [movieCrew, setMovieCrew] = useState<Crew[]>([]);\n\n  async function handleMovieDetail(movieId: string) {\n    const { data } = await getMovieDetail(movieId);\n    setMovieDetail(data);\n  }\n\n  async function handleMovieSocial(movieId: string) {\n    const { data } = await getMovieSocial(movieId);\n    setMovieSocial(data);\n  }\n\n  async function handleMovieCast(movieId: string) {\n    const { data } = await getMovieCastAndCrew(movieId);\n    setMovieCast(data.cast);\n  }\n\n  async function handleMovieCrew(movieId: string) {\n    const { data } = await getMovieCastAndCrew(movieId);\n    setMovieCrew(data.crew);\n  }\n\n  // async function handleMovieGenre(movieId: string) {\n  //   const { data } = await getMovieGenre(movieId);\n  //   console.log(data.genres);\n  // }\n\n  useEffect(() => {\n    handleMovieDetail(params.movieId);\n    handleMovieSocial(params.movieId);\n    handleMovieCast(params.movieId);\n    handleMovieCrew(params.movieId);\n  }, [setMovieDetail, setMovieSocial, setMovieCast, setMovieCrew]);\n\n  return (\n    <>\n      <Menu />\n      {movieDetail && (\n        <S.Details>\n          <img\n            src={`${process.env.REACT_APP_TMDB_POSTER}${movieDetail.poster_path}`}\n            alt={movieDetail.title}\n          />\n          <section>\n            <header>\n              <strong>{movieDetail.title}</strong>\n              <FI.FiStar size={20} />\n              <p>{movieDetail.original_title}</p>\n            </header>\n\n            <div>\n              <span>{movieDetail.release_date}</span>\n              <span>{movieDetail.runtime} min</span>\n              <p id=\"tagline\">{movieDetail.tagline}</p>\n              <p id=\"sinopse\">{movieDetail.overview}</p>\n            </div>\n\n            <footer>\n              {movieDetail.genres.map(genre => (\n                <span key={genre.id}>{genre.name}</span>\n              ))}\n              <ul>\n                <li>\n                  <strong>Direção</strong>\n                  {movieCrew &&\n                    movieCrew.map(crew => {\n                      return (\n                        <p key={crew.credit_id}>\n                          {crew.job === 'Director' && crew.name}\n                        </p>\n                      );\n                    })}\n                </li>\n                <li>\n                  <strong>Elenco</strong>\n                  {movieCast &&\n                    movieCast.slice(0, 3).map(cast => {\n                      return <p key={cast.id}>{cast.name}</p>;\n                    })}\n                </li>\n              </ul>\n              {movieSocial && (\n                <div>\n                  {movieSocial.instagram_id && (\n                    <a\n                      href={`https://instagram.com/${movieSocial.instagram_id}`}\n                      target=\"_blank\"\n                      rel=\"noreferrer\"\n                    >\n                      <FI.FiInstagram size={20} />\n                    </a>\n                  )}\n\n                  {movieSocial.facebook_id && (\n                    <a\n                      href={`https://facebook.com/${movieSocial.facebook_id}`}\n                      target=\"_blank\"\n                      rel=\"noreferrer\"\n                    >\n                      <FI.FiFacebook size={20} />\n                    </a>\n                  )}\n\n                  {movieSocial.twitter_id && (\n                    <a\n                      href={`https://twitter.com/${movieSocial.twitter_id}`}\n                      target=\"_blank\"\n                      rel=\"noreferrer\"\n                    >\n                      <FI.FiTwitter size={20} />\n                    </a>\n                  )}\n                </div>\n              )}\n            </footer>\n          </section>\n        </S.Details>\n      )}\n    </>\n  );\n};\n\nexport default Movie;\n","/home/parvic/dev/projects/tmdb-web/src/pages/Movie/style.ts",[],"/home/parvic/dev/projects/tmdb-web/src/services/api.ts",[],"/home/parvic/dev/projects/tmdb-web/src/services/movie-services.ts",["111","112","113","114","115"],"import api from './api';\n\nexport const getTrendingMovies = async () => {\n  try {\n    const { data, status } = await api.get(\n      `/discover/movie?api_key=${process.env.REACT_APP_TMDB_KEY}&language=pt-BR&sort_by=popularity.desc&page=1`,\n    );\n    return { data, status };\n  } catch (error) {\n    return { error };\n  }\n};\n\nexport const getMovieDetail = async (movieId: string) => {\n  try {\n    const { data, status } = await api.get(\n      `/movie/${movieId}?api_key=${process.env.REACT_APP_TMDB_KEY}&language=pt-BR`,\n    );\n    return { data, status };\n  } catch (error) {\n    return { error };\n  }\n};\n\nexport const getMovieCastAndCrew = async (id: string) => {\n  try {\n    const { data, status } = await api.get(\n      `/movie/${id}/credits?api_key=${process.env.REACT_APP_TMDB_KEY}`,\n    );\n    return { data, status };\n  } catch (error) {\n    return { error };\n  }\n};\n\nexport const getMovieSocial = async (id: string) => {\n  try {\n    const { data, status } = await api.get(\n      `/movie/${id}/external_ids?api_key=${process.env.REACT_APP_TMDB_KEY}`,\n    );\n    return { data, status };\n  } catch (error) {\n    return { error };\n  }\n};\n\nexport const getSearchResult = async (movieName: string) => {\n  try {\n    const { data, status } = await api.get(\n      `search/movie?api_key=${process.e